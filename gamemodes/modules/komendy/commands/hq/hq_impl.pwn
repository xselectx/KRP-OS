//------------------<[ Implementacja: ]>-------------------
command_hq_Impl(playerid)
{
	if(IsPlayerInGroup(playerid, 1) || PlayerInfo[playerid][pLider] == 1 || IsPlayerInGroup(playerid, 3) || PlayerInfo[playerid][pLider] == 3)
	{
		ShowHeadquarters(playerid);
	}
	else
	{
		return noAccessMessage(playerid);
	}
    return 1;
}

