//-----------------<[ Funkcje: ]>-------------------
public Czaspodnoszenia(playerid)
{

	SetPlayerAttachedObject(playerid, 0, 1220, 1, 0.207000, 0.811999, 0.0, 0.0, 86.40, 177.199996, 1.0, 1.0, 1.0);
	TogglePlayerControllable(playerid, 1);
	SetPlayerSpecialAction(playerid,SPECIAL_ACTION_CARRY);
	ClearAnimations(playerid);
	MaPaczke[playerid] = 1;
	ACMagazynier[playerid] = gettime() + 10;
	SendClientMessage(playerid, COLOR_BIALY, "[Magazynier] Pomyslnie podniosles paczke.");
	return 1;
}
