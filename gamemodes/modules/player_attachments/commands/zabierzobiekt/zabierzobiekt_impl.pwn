//------------------<[ Implementacja: ]>-------------------
command_zabierzobiekt_Impl(playerid, giveplayerid)
{
    if(PlayerInfo[playerid][pAdmin] < 1)
    {
        sendErrorMessage(playerid, "Nie jesteœ uprawniony!");
        return 1;
    }

    DialogAdminRemoveAttachedItems(playerid, giveplayerid);
    return 1;
}

