	{
		if(PlayerInfo[playerid][pAdmin] >= 1 || IsAScripter(playerid))
		{
			sendTipMessage(playerid, "Nie s³yszysz ju¿ nic! Nie widzisz ju¿ nic! Taki z ciebie admin"); 
			DeathWarning[playerid] = 0;
