YCMD:przelew_telefon(playerid, params[], help)
{
    new phone, money;
    if(sscanf(params, "dd", phone, money))
        return sendTipMessage(playerid, "U¿yj: /przelew_telefon [numer telefonu] [kasa]");
    PhoneTransfer(playerid, phone, money);
    return 1;
}