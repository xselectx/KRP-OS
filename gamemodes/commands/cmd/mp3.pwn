YCMD:mp3(playerid, params[], help)
{
    if(IsPlayerConnected(playerid))
    {
        if(HasItemType(playerid, ITEM_TYPE_MP3) != -1)
			return ShowPlayerDialogEx(playerid, DIALOGID_MUZYKA, DIALOG_STYLE_LIST, "Odtwarzacz MP3.","Radio\n""W³asny stream\n""MP3 OFF","Start",""); //zmieñ dialogid
		else
			return sendTipMessageEx(playerid, COLOR_GREY, "Nie posiadasz odtwarzacza mp3!");
    }
    return 1;
}
