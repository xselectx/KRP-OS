YCMD:ubranie(playerid, params[], help)
{
    if(IsPlayerConnected(playerid))
	{
	    if(IsAtClothShop(playerid))
	    {
            //NowaWybieralka_Setup(playerid);
			RunCommand(playerid, "kup", "");
		}
		else
		{
		    sendErrorMessage(playerid, "Nie jesteœ w sklepie z ubraniami!");
		}
	}
	return 1;
}
