YCMD:edytuj(playerid, params[], help)
{
    if(PlayerInfo[playerid][pAdmin] < 1 && PlayerInfo[playerid][pZG] < 1 && PlayerInfo[playerid][pNewAP] < 1 && !IsAScripter(playerid)) return 1;
    ShowPlayerDialogEx(playerid, D_EDIT, DIALOG_STYLE_LIST, "Panel Boga", "Wybierz co chcesz edytowaæ:\n1.\tPojazd", "Wybierz", "WyjdŸ");
    return 1;
}
