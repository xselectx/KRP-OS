YCMD:wywaltelefon(playerid, params[], help) {
    if(IsPlayerConnected(playerid) && GetPhoneNumber(playerid) > 0) {
		Item_Destroy(playerid, HasItemType(playerid, ITEM_TYPE_PHONE), 1);
		MSGBOX_Show(playerid, "Telefon ~g~Wywalony", MSGBOX_ICON_TYPE_OK);
	}
	else 
	{
		sendErrorMessage(playerid, "Nie możesz tego zrobić!");
	}
    return 1;
}
