YCMD:loadinfo(playerid, params[], help)
{
    if(!Uprawnienia(playerid, ACCESS_OWNER)) return 1;
    SendClientMessage(playerid, -1, "Wczytujê informacje...");
    LoadServerInfo();
    SendClientMessage(playerid, -1, "OK");
    return 1;
}
