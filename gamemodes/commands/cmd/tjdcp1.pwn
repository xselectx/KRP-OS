YCMD:tjdcp1(playerid, params[], help)
{
    if(PlayerInfo[playerid][pAdmin] < 5) return 1;
    new idx;
    sscanf(params, "d", idx);
    new id = GetPlayerVehicleID(playerid);
    SetVehiclePos(id, TransportJobData[idx][eTJDStartX], TransportJobData[idx][eTJDStartY], TransportJobData[idx][eTJDStartZ]+1.0);
    PutPlayerInVehicleEx(playerid, id,0);
    return 1;
}
