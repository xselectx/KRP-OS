YCMD:kradnij(playerid, params[], help)
{
	new itemid = HasItemType(playerid, ITEM_TYPE_WYTRYCH);
	if(itemid == -1) return sendErrorMessage(playerid, "Nie masz wytrychu!");
	else Item_Use(playerid, itemid);
	return 1;
}
