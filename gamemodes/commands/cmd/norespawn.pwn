{
    if(gLastCar[playerid] > 0)
    {
        sendTipMessage(playerid, sprintf("Twój pojazd o ID %d nie zrespawnuje siê podczas respawnu!", gLastCar[playerid]));
        NoRespawn[gLastCar[playerid]] = 1;
    } else return sendErrorMessage(playerid, "Nie siedzia³eœ ostatnio w ¿adnym pojeŸdzie");
    return 1;
}