YCMD:newstype(playerid, params[], help)
{
	if(newsTypePlayer[playerid] >= 3)
	{
		newsTypePlayer[playerid] = 0; 
		MSGBOX_Show(playerid, "Chat_News:_~g~Chat", MSGBOX_ICON_TYPE_WARNING); 
		PlayerTextDrawHide(playerid, SN_MESS[playerid]); 
		return 1;
	}
	newsTypePlayer[playerid]++ ;
	if(newsTypePlayer[playerid] == 2)
	{
		MSGBOX_Show(playerid, "Chat_News:_~g~TextDraw", MSGBOX_ICON_TYPE_WARNING); 
	}
	else if(newsTypePlayer[playerid] == 3)
	{
		MSGBOX_Show(playerid, "Chat_News:_~g~Wszedzie", MSGBOX_ICON_TYPE_WARNING); 
	}
	return 1;
}
