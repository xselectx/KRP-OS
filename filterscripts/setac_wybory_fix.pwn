#include <a_samp>
#include <nex-ac>

//-----------------<[ Callback'i: ]>-------------------
public OnFilterScriptInit()
{
	EnableAntiCheat(39, 0);
	
	for(new i; i<MAX_PLAYERS; i++)
	{
		EnableAntiCheatForPlayer(i, 39, false);
	}

	print("\n--------------------------------------");
	print(" Fliterscript 'setac_wybory_fix' zaladowany");
	print("--------------------------------------\n");
	return 1;
}

public OnFilterScriptExit()
{
	
	EnableAntiCheat(39, 1);
	
	print("\n--------------------------------------");
	print(" Fliterscript 'setac_wybory_fix' wylaczony");
	print("--------------------------------------\n");
	return 1;
}